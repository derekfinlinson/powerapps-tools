    "lint": "eslint",
    "clean": "rimraf lib"
  },
  "author": "Derek Finlinson",
  "keywords": [
    "dataverse",
    "dynamics 365",
    "dynamics 365",
    "webapi",
    "xrm",
    "typescript",
    "d365",
    "common data service"
  ],
  "devDependencies": {
    "@types/xrm": "^9.0.86"
  },
  "volta": {
    "extends": "../../package.json"
  }
