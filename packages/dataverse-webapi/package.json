{
  "name": "dataverse-webapi",
  "version": "3.1.1",
  "description": "Dataverse Web Api TypeScript Module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      }
    },
    "./lib/node": {
      "import": {
        "types": "./lib/node.d.ts",
        "default": "./lib/node.js"
      }
    }
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/derekfinlinson/powerapps-tools"
  },
  "scripts": {
